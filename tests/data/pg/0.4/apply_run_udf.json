{
  "collection": {
    "process_id": "load_collection",
    "arguments": {
      "id": "S2_FAPAR_CLOUDCOVER"
    }
  },
  "apply": {
    "process_id": "apply",
    "arguments": {
      "data": {
        "from_node": "collection"
      },
      "process": {
        "callback": {
          "abs": {
            "arguments": {
              "data": {
                "from_argument": "data"
              }
            },
            "process_id": "abs"
          },
          "udf": {
            "arguments": {
              "data": {
                "from_node": "abs"
              },
              "runtime": "Python",
              "version": "3.6",
              "udf": "my python code"
            },
            "process_id": "run_udf",
            "result": true
          }
        }
      }
    },
    "result": true
  }
}