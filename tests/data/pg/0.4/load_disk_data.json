{
  "loaddiskdata": {
    "process_id": "load_disk_data",
    "arguments": {
      "format": "GTiff",
      "glob_pattern": "/data/MTDA/CGS_S2/CGS_S2_FAPAR/2019/04/24/*/*/10M/*_FAPAR_10M_V102.tif",
      "options": {
        "date_regex": "_(\\d{4})(\\d{2})(\\d{2})T"
      }
    }
  },
  "filterbbox": {
    "process_id": "filter_bbox",
    "arguments": {
      "data": {
        "from_node": "loaddiskdata"
      },
      "extent": {
        "west": 3,
        "east": 6,
        "south": 50,
        "north": 51,
        "crs": "EPSG:4326"
      }
    },
    "result": true
  }
}